
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
//import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class EcommercePractices {

	public static void main(String[] args) {
	
		System.setProperty("webdriver.chrome.driver","C:\\chromedriver\\chromedriver_win32\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("https://www.rahulshettyacademy.com/AutomationPractice/");
		driver.findElement(By.id("twotabsearchtextbox")).sendKeys("toys");
		driver.findElement(By.className("s-suggestion")).click();
		//List<WebElement> elements = driver.findElements(By.xpath("//ul[@class='erkvQe']//li//span"));

	}

}
 